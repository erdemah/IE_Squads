<body>
<%include header.ejs%>

<!--  TILES CODE-->
<div class="container">
    <div class="row">
        <div class="tile">
            <div class="tile-thumbnail" onclick="location.href='annual_bonus';" style="cursor: pointer;">
                <img src="images/annual_bonus.jpg" alt="My Awesome Tile">
            </div>
        </div>
        <div class="tile">
            <div class="tile-thumbnail" onclick="location.href='pay_slip';" style="cursor: pointer;">
                <img src="images/pay_slips.jpg" alt="My Awesome Tile">
            </div>
        </div>
        <div class="tile">
            <div class="tile-thumbnail" onclick="location.href='annual_bonus';" style="cursor: pointer;">
                <img src="images/annual_bonus.jpg" alt="My Awesome Tile">
            </div>
        </div>
        <div class="tile">
            <div class="tile-thumbnail" onclick="location.href='pay_rate_level';" style="cursor: pointer;">
                <img src="images/pay_rate_level.jpg" alt="My Awesome Tile">
            </div>
        </div>
    </div>
</div>
<!-- TILES CODE END-->

<!-- CAROUSEL CODE-->
<div class="bs-example">
    <div id="myCarousel" class="carousel slide" data-interval="3000" data-ride="carousel">
        <!-- Carousel indicators -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>
        <!-- Wrapper for carousel items -->
        <div class="carousel-inner">
            <div class="carousel-item active">
                <div class="numbertext">1 / 5</div>
                <img src="images/annualleave/fulltime/1.png" alt="First Slide">
                <div class="carousel-caption d-none d-md-block">
                </div>
            </div>
            <div class="carousel-item">
                <div class="numbertext">2 / 5</div>
                <img src="images/annualleave/fulltime/2.png" alt="Second Slide">
                <div class="carousel-caption d-none d-md-block">
                </div>
            </div>
            <div class="carousel-item">
                <div class="numbertext">3 / 5</div>
                <img src="images/annualleave/fulltime/3.png" alt="Third Slide">
                <div class="carousel-caption d-none d-md-block">
                </div>
            </div>
            <div class="carousel-item">
                <div class="numbertext">4 / 5</div>
                <img src="images/annualleave/fulltime/4.png" alt="Third Slide">
                <div class="carousel-caption d-none d-md-block">
                </div>
            </div>
            <div class="carousel-item">
                <div class="numbertext">5 / 5</div>
                <img src="images/annualleave/fulltime/5.png" alt="Third Slide">
                <div class="carousel-caption d-none d-md-block">
                </div>
            </div>
        </div>
        <!-- Carousel controls -->
        <a class="carousel-control-prev" href="#myCarousel" data-slide="prev">
            <span class="carousel-control-prev-icon"></span>
        </a>
        <a class="carousel-control-next" href="#myCarousel" data-slide="next">
            <span class="carousel-control-next-icon"></span>
        </a>
    </div>
</div>
</body>
<!-- CAROUSEL CODE END-->
<%include links.ejs%>
